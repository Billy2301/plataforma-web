@model TriajeOnlineViewModel
@{
    ViewData["title"] = "Cambiar Contraseña";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    .tooltip {
        --bs-tooltip-bg: #fff!important;
        opacity: 100%!important;
    }
    .tooltip-inner {
        box-shadow: rgba(0, 0, 0, 0.24) 0px 3px 8px;
        max-width: 500px!important;
        color: #224078;
        text-align: justify;
        background-color: #fff!important;
        opacity: 100%;
    }

    .cpal-box > i {
        opacity: 0.3;
    }

    .cpal-box:hover > i {
        opacity: 1;
    }
</style>
<div class="card bg-info-subtle shadow-none position-relative overflow-hidden mb-4">
    <div class="card-body px-4 py-3">
        <div class="row align-items-center">
            <div class="col-9">
                <h4 class="fw-semibold mb-8 text-primary">
                    Formulario de atención
                </h4>
                <nav aria-label="breadcrumb">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item">
                            <a class="text-muted text-decoration-none" asp-action="Index" asp-controller="Home">Inicio</a>
                        </li>
                        <li class="breadcrumb-item" aria-current="page">Formulario de atención</li>
                    </ol>
                </nav>
            </div>
            <div class="col-3">
                <div class="text-center">
                    <svg xmlns="http://www.w3.org/2000/svg" width="2.5em" height="2.5em" viewBox="0 0 24 24" class="cc BTC text-primary fs-7">
                        <g fill="none" stroke="currentColor" stroke-linecap="round" stroke-width="2">
                            <!-- Primer trazo -->
                            <path d="M9 5H7a2 2 0 0 0-2 2v12a2 2 0 0 0 2 2h10a2 2 0 0 0 2-2V7a2 2 0 0 0-2-2h-2" stroke-dasharray="62" stroke-dashoffset="62">
                                <animate fill="freeze" attributeName="stroke-dashoffset" begin="0.7s" dur="0.5s" values="62;0" />
                            </path>
                            <!-- Segundo trazo -->
                            <path d="M9 5a2 2 0 0 1 2-2h2a2 2 0 0 1 2 2v0a2 2 0 0 1-2 2h-2a2 2 0 0 1-2-2m1 9h4m-2-2v4" stroke-dasharray="44" stroke-dashoffset="44">
                                <animate fill="freeze" attributeName="stroke-dashoffset" begin="0.7s" dur="0.5s" values="44;0" />
                            </path>
                        </g>
                    </svg>
                </div>
            </div>
        </div>
    </div>
</div>

 <!-- Vertically centered modal -->
<div class="modal fade" id="modalDatos" tabindex="-1" aria-labelledby="vertical-center-modal"
     aria-hidden="true">
    <div class="modal-dialog modal-sm modal-dialog-centered">
        <div class="modal-content modal-filled bg-warning-subtle">
            <div class="modal-body p-4">
                <div class="text-center text-warning">
                    <i class="ti ti-alert-octagon fs-10"></i>
                    <h4 class="mt-2 text-warning">Verifique esta Información</h4>
                    <div class="row text-start text-muted">
                        <div class="col-12 mb-2">
                            <b><small>Nombres :</small></b><span class="ms-3">@Model.Tab1.NombresPaciente?.ToUpper()</span>
                        </div>
                        <div class="col-12 mb-2">
                            <b><small>Apellidos :</small></b><span class="ms-3">@Model.Tab1.ApellidoPaternoPaciente?.ToUpper() @Model.Tab1.ApellidoMaternoPaciente?.ToUpper()</span>
                        </div>
                        <div class="col-12 mb-2">
                            <b><small>fecha nacimiento :</small></b><span class="ms-3">@(Model.Tab1.FechaNacimientoPaciente != null ? Convert.ToDateTime(Model.Tab1.FechaNacimientoPaciente).ToString("dd 'de' MMMM 'de' yyyy"): "")</span>
                        </div>
                        <div class="col-6 mb-2">
                            <b>@Model.Tab1.TipoDocumento?.ToUpper() :</b><span class="ms-1">@Model.Tab1.PacienteDNI</span>
                        </div>
                        <div class="col-6 mb-2 p-0">
                            <b><small>Sexo :</small></b><span class="ms-1">@(Model.Tab1.SexoPaciente == 1 ? "MASCULINO" : "FEMENINO")</span>
                        </div>
                        <div class="col-12 mb-2">
                            <b><small>Colegio :</small></b><span class="ms-3">@Model.Tab1.Colegio?.ToUpper()</span>
                        </div>
                    </div>
                    <button type="button" class="btn btn-muted btn-sm my-2" data-bs-dismiss="modal">
                        Modificar <i class="ti ti-pencil-plus fs-4"></i>
                    </button>
                    <button type="button" class="btn btn-warning btn-sm my-2" data-bs-dismiss="modal" onclick="datosOK()">
                        Es correcto <i class="ti ti-check fs-4"></i>
                    </button>
                </div>
            </div>
        </div>
        <!-- /.modal-content -->
    </div>
</div>

<div class="row">
    <div class="col-12"> 
        <!--  start Step wizard with validation -->
        <div class="card">
            <div class="p-1 p-md-5 wizard-content">

                @if (ViewBag.Exito != null)
                {
                    <script>
                        document.addEventListener("DOMContentLoaded", function (event) {
                            Swal.fire({
                                title: "Genial !",
                                text: "@(System.Net.WebUtility.HtmlDecode(ViewBag.Exito))",
                                icon: "success",
                                allowOutsideClick : false,
                                allowEscapeKey : false,
                                allowEnterKey : false,
                                confirmButtonText: '<i class="fas fa-home"></i> Volver a Inicio!'
                            }).then((result) => {
                                if (result.isConfirmed) {
                                    window.location.href = '@Url.Action("Index", "Home")';
                                } else if (result.isDenied) {
                                    window.location.href = '@Url.Action("Index", "Home")';
                                }
                            });
                        });
                    </script>
                }
                @if (ViewBag.Error != null)
                {
                    <script>
                        document.addEventListener("DOMContentLoaded", function (event) {
                            Swal.fire({
                                title: "Error !",
                                text: "@(System.Net.WebUtility.HtmlDecode(ViewBag.Error))",
                                icon: "error"
                            });
                        });
                    </script>
                }
                @if(ViewBag.datos != null){
                    <script>
                        document.addEventListener("DOMContentLoaded", function (event) {
                            setTimeout(function() {
                                $("#modalDatos").modal("show");
                            }, 1000);
                        });
                    </script>
                   
                }

                <form asp-controller="TriajeOnline" asp-action="Triaje" method="post" id="formTriaje" class="validation-wizard  wizard-circle mt-5">
                    @* validation-wizard *@

                    <div class="text-danger" asp-validation-summary="All" role="alert">

                    </div>
                    <input type="hidden"  asp-for="Tab1.UsuarioId">
                    <input type="hidden" asp-for="Tab1.Hc">
                    
                    <!-- Step 1 -->
                    <h6 class="d-none d-md-block">DATOS</h6>
                    <section>
                        <hr />
                        <div class="d-md-flex mt-md-5">
                            <div class="col-md-4 me-4" style="text-align:justify">
                                <h4 class="text-primary">
                                    <strong>
                                        @* Ingrese por favor los datos de la persona en consulta (pec) *@
                                        Por favor, ingrese los datos de la persona que se atenderá con nosotros.
                                    </strong>
                                </h4>
                                <p>
                                    En esta sección queremos conocer a la
                                    persona que recibirá nuestra atención.
                                    Por ello, nos gustaría que se <b>
                                        completen
                                        los campos con la información requerida.
                                    </b>
                                </p>
                            </div>
                            <div class="row col-md-8">
                                <div class="col-md-6 mb-3">
                                    <input type="text" id="txtNombrePaciente" class="form-control upperCase border border-primary text-primary " asp-for="Tab1.NombresPaciente" placeholder="Nombres de la Persona en atenderse" >
                                    <span asp-validation-for="Tab1.NombresPaciente" class="text-danger"></span>

                                </div>
                                <div class="col-md-6 mb-3">
                                    <input type="text" id="txtApPaternoPaciente" class="form-control upperCase border border-primary text-primary " asp-for="Tab1.ApellidoPaternoPaciente" placeholder="Ape. Paterno de la Persona en atenderse" >
                                    <span asp-validation-for="Tab1.ApellidoPaternoPaciente" class="text-danger"></span>

                                </div>
                                <div class="col-md-6 mb-3">
                                    <input type="text" id="txtApMaternoPaciente" class="form-control upperCase border border-primary text-primary " asp-for="Tab1.ApellidoMaternoPaciente" placeholder="Ape. Materno de la Persona en atenderse" >
                                    <span asp-validation-for="Tab1.ApellidoMaternoPaciente" class="text-danger"></span>
                                </div>
                                <div class="col-md-6 mb-3">
                                    <select class="form-select border border-primary text-primary border-primary text-primary" asp-for="Tab1.SexoPaciente" required="">
                                        <option value="0" disabled>--SELECCIONAR SEXO--</option>
                                        <option value="1">MASCULINO</option>
                                        <option value="2">FEMENINO</option>
                                    </select>
                                    <span asp-validation-for="Tab1.SexoPaciente" class="text-danger"></span>
                                </div>
                                <div class="col-md-6 mb-3">
                                    <select class="form-select border border-primary text-primary " asp-for="Tab1.TipoDocumento" required="" onchange="verifyTipoDoc()">
                                        <option value="" disabled>--SELECCIONE TIPO DOC.--</option>
                                        <option value="DNI">DNI</option>
                                        <option value="CARNET_EXT">CARNET DE EXTRANJERIA</option>
                                    </select>
                                    <span asp-validation-for="Tab1.TipoDocumento" class="text-danger"></span>

                                </div>
                                <div class="col-md-6 mb-3">
                                    <input type="text" id="txtDniPaciente" class="form-control upperCase border border-primary text-primary " asp-for="Tab1.PacienteDNI" placeholder="Nro Doc. de la Persona en atenderse" required="">
                                    <span asp-validation-for="Tab1.PacienteDNI" class="text-danger"></span>
                                    <span id="nroDocValidationMessage" class="text-danger"></span>
                                </div>
                                <div class="col-md-6 mb-3">
                                    <select class="form-select border border-primary text-primary " asp-for="Tab1.Procedencia" required="">
                                            <option selected="selected" value="" disabled="true">-- SELEC. PROCEDENCIA --</option>
                                            <option value="LIMA">LIMA</option>
                                            <option value="PROVINCIA">PROVINCIA</option>
                                            <option value="EXTRANJERO">EXTRANJERO</option>
                                    </select>
                                    <span asp-validation-for="Tab1.Procedencia" class="text-danger"></span>
                                </div>
                                <div class="col-md-6 mb-3">
                                    <input type="text" class="form-control upperCase border border-primary text-primary " asp-for="Tab1.FechaNacimientoPaciente" id="txtFechaNacimientoPaciente" placeholder="Fecha de Nac. de la Persona en atenderse" required="" pattern="^(0[1-9]|[1-2][0-9]|3[0-1])/(0[1-9]|1[0-2])/[0-9]{4}$">
                                    <span asp-validation-for="Tab1.FechaNacimientoPaciente" class="text-danger"></span>
                                    <span id="fechaNacValidationMessage" class="text-danger"></span>
                                </div>
                                <div class="col-md-6 mb-3">
                                    <select class="form-select border border-primary text-primary " asp-for="Tab1.NivelEducacion" required="" onchange="mustShowGrado(this)">
                                        <option value="" disabled>--SELECCIONE NIVEL EDUCATIVO--</option>
                                        <option value="INICIAL">INICIAL</option>
                                        <option value="PRIMARIA">PRIMARIA</option>
                                        <option value="SECUNDARIA">SECUNDARIA</option>
                                        <option value="SUPERIOR">SUPERIOR</option>
                                        <option value="OTROS">OTROS</option>
                                    </select>
                                    <span asp-validation-for="Tab1.NivelEducacion" class="text-danger"></span>
                                </div>
                                <div class="col-md-6 mb-3 d-flex">
                                    <div class="col-8 m-0 pe-2">
                                        <input type="text" class="form-control upperCase border border-primary text-primary " asp-for="Tab1.Colegio" placeholder="Centro educativo" required="">
                                        <span asp-validation-for="Tab1.Colegio" class="text-danger"></span>
                                    </div>
                                    <div class="col-4 m-0">
                                        <input type="text" class="form-control upperCase border border-primary text-primary " asp-for="Tab1.PacienteGrado" placeholder="Grado">
                                        <span asp-validation-for="Tab1.PacienteGrado" class="text-danger"></span>

                                    </div>
                                </div>
                            </div>
                        </div>
                        <hr />
                    </section>
                    <!-- Step 2 -->
                    <h6 class="d-none d-md-block">CONTACTO</h6>
                    <section>
                        <hr />
                        <div class="row mt-md-5">
                            <div class="col-md-9">
                                <h4 class="text-primary">¿La persona que se atenderá depende de un adulto?</h4>
                            </div>
                            <div class="col-md-3">
                                    <div class="form-check form-check-inline">
                                    <input class="form-check-input warning" type="radio" value="SI" asp-for="Tab2.Dependiente" id="cbxDependiente" onchange="isCheckedDependiente()">
                                        <label class="form-check-label" for="cbxDependiente"><b>SI</b></label>
                                    </div>
                                    <div class="form-check form-check-inline">
                                    <input class="form-check-input warning" type="radio" value="NO" asp-for="Tab2.Dependiente" id="cbxDependienteNo" onchange="isCheckedDependiente()">
                                    <label class="form-check-label" for="cbxDependienteNo"><b>NO</b></label>
                                    </div>
                                    <span asp-validation-for="Tab2.Dependiente" class="text-danger"></span>
                             </div>
                        </div>
                        <div class="d-sm-flex mt-md-5">
                            <div class="col-md-4 me-md-4">
                                <p class="m-0">Por favor bríndenos los datos del apoderado o persona de contacto</p>
                            </div>
                            <div class="row col-md-8">
                                <div class="col-md-6 mb-3">
                                    <input type="text" class="form-control border border-primary upperCase text-primary " asp-for="Tab2.ApoderadoTelCelular" placeholder="Nro Teléfono de contacto" >
                                    <span asp-validation-for="Tab2.ApoderadoTelCelular" class="text-danger"></span>
                                </div>
                                <div class="col-md-6 mb-3">
                                    <input type="email" class="form-control border border-primary text-primary upperCase input-disabled" asp-for="Tab2.EmailRegistro" placeholder="Email de de contacto">
                                    <span asp-validation-for="Tab2.EmailRegistro" class="text-danger"></span>
                                </div>
                            </div>
                        </div>
                        <div class="d-sm-flex" id="targetDiv">
                            <div class="col-md-4 me-md-4 pt-3">
                                <p>
                                    <b>
                                        ¿Parentesco con la persona a atenderse?
                                    </b>
                                </p>
                                <div class="mb-3">
                                    <select id="sltApoderadoRelacion" class="form-select border border-primary text-primary" asp-for="Tab2.ApoderadoRelacion">
                                            <option selected="selected" value="" disabled="true">-- SELECCIONAR RELACION --</option>
                                            <option value="MAMÁ">MAMÁ</option>
                                            <option value="PAPÁ">PAPÁ</option>
                                            <option value="TÍO(A)">TÍO(A)</option>
                                            <option value="ABUELO(A)">ABUELO(A)</option>
                                            <option value="PRIMO(A)">PRIMO(A)</option>
                                            <option value="HERMANO(A)">HERMANO(A)</option>
                                            <option value="SOBRINO(A)">SOBRINO(A)</option>
                                            <option value="OTROS">OTROS</option>
                                    </select>
                                    <span asp-validation-for="Tab2.ApoderadoRelacion" class="text-danger"></span>
                                </div>
                                <div class="mb-3">
                                    <select id="sltContactoSexo" class="form-select border border-primary text-primary " asp-for="Tab2.ContactoSexo" required="">
                                        <option selected="selected" value="" disabled="true">-- SELECCIONAR SEXO --</option>
                                        <option value="1">MASCULINO</option>
                                        <option value="2">FEMENINO</option>
                                    </select>
                                    <span asp-validation-for="Tab2.ContactoSexo" class="text-danger"></span>
                                </div>
                            </div>
                            <div class="row col-md-8">
                                <div class="col-md-6 mb-3">
                                    <input type="text" id="txtApoderadoNombre" class="form-control upperCase border border-primary text-primary " asp-for="Tab2.ApoderadoNombre" placeholder="Nombre del Apoderado">
                                    <span asp-validation-for="Tab2.ApoderadoNombre" class="text-danger"></span>
                                </div>
                                <div class="col-md-6 mb-3">
                                    <input type="text" id="txtApoderadoApellidoPaterno" class="form-control upperCase border border-primary text-primary " asp-for="Tab2.ApoderadoApellidoPaterno" placeholder="Ap. paterno del Apoderado">
                                    <span asp-validation-for="Tab2.ApoderadoApellidoPaterno" class="text-danger"></span>
                                </div>
                                <div class="col-md-6 mb-3">
                                    <input type="text" id="txtApoderadoApellidoMaterno" class="form-control upperCase border border-primary text-primary " asp-for="Tab2.ApoderadoApellidoMaterno" placeholder="Ap. materno del Apoderado">
                                    <span asp-validation-for="Tab2.ApoderadoApellidoMaterno" class="text-danger"></span>
                                </div>
                                
                                <div class="col-md-6 mb-3">
                                    <input type="text" id="txtContactoFechaNac" class="form-control upperCase border border-primary text-primary " asp-for="Tab2.ContactoFechaNac" id="txtContactoFechaNac" placeholder="Fecha de Nacimiento del apoderado">
                                    <span asp-validation-for="Tab2.ContactoFechaNac" class="text-danger"></span>
                                    <span id="FechaNacContValidationMessage" class="text-danger"></span>
                                </div>
                                <div class="col-md-6 mb-3">
                                    <select id="sltContactoTipoDoc" class="form-select border border-primary text-primary" asp-for="Tab2.ContactoTipoDoc" onchange="verifyTipoDocCont()">
                                        <option selected="selected" value="" disabled="true">-- SELEC. TIPO DE DOCUMENTO --</option>
                                        <option value="DNI">DNI</option>
                                        <option value="PASAPORTE">PASAPORTE</option>
                                        <option value="CARNET_EXT">CARNET DE EXTRANJERIA</option>
                                    </select>
                                    <span asp-validation-for="Tab2.ContactoTipoDoc" class="text-danger"></span>

                                </div>
                                <div class="col-md-6 mb-3">
                                    <input type="text" id="txtContactoNroDoc" class="form-control upperCase border border-primary text-primary " asp-for="Tab2.ContactoNroDoc" placeholder="Nro Documento del apoderado">
                                    <span asp-validation-for="Tab2.ContactoNroDoc" class="text-danger"></span>
                                    <span id="nroDocContValidationMessage" class="text-danger"></span>
                                </div>

                                
                            </div>
                            
                        </div>
                        <div class="col-12 d-flex mt-3 @(Model.Tab2.GuardarEnPerfil ? "" : "d-none")">
                            <label class="form-check-label" for="flexSwitchCheckDefault"><h5 class="text-primary">Guardar estos datos en la información de mi perfil</h5></label>
                            <div class="form-check form-switch ms-5 pt-3 pt-md-0">
                                <input class="form-check-input" type="checkbox" asp-for="Tab2.GuardarEnPerfil" id="cbxGuadarEnPerfil">
                            </div>
                        </div>
                        <hr />
                    </section>
                    <!-- Step 3 -->
                    <h6 class="d-none d-md-block">ESPECIALIDAD</h6>
                    <section>
                        <hr />
                        <div class="row">
                            <h4 class="text-primary">
                                <strong>
                                    Por favor, seleccione la especialidad o especialidades en las que desea obtener una cita. (Puede seleccionar más de una).
                                </strong>
                            </h4>
                        </div>
                        <div class="row mt-md-5">
                            <div class="col-md-4 py-2">
                                <input type="checkbox" class="btn-check" id="cbx_Lenguaje" autocomplete="off" asp-for="Tab3.EvaluarLenguaje">
                                <label class="cpal-box btn btn-outline-primary d-flex align-items-center p-y4" id="box_Lenguaje" for="cbx_Lenguaje" onclick="evaluacionSelect('cbx_Lenguaje','Lenguaje')" style="min-height: 100px">
                                    <a href="JavaScript: void(0);">
                                        <img src="~/assets/images/iconeval/Lenguaje.svg" alt="" width="50" height="50" />
                                    </a>
                                    <div class="ms-3 mt-2">
                                        <h5>LENGUAJE</h5>
                                    </div>
                                    <i class="fa fa-info-circle ms-auto fs-7" data-bs-html="true" data-bs-toggle="tooltip" data-bs-placement="top" aria-label="Lenguaje" data-bs-original-title="<b>LENGUAJE</b><hr> Atiende a niños que se relacionan bien con las personas y se dejan entender con gestos, pero demoran en empezar a hablar. Igualmente, niños y adolescentes que tienen poco lenguaje o no comprenden lo que se les dice, su vocabulario es pobre, no construyen bien las oraciones, les cuesta organizar sus ideas y hacerse entender por los demás.<br> No se ve en esta área casos de niños con indicadores de autismo, ingresar al área de Alteraciones de Desarrollo. </div>"></i>
                                </label>
                            </div>
                            <div class="col-md-4 py-2">
                                <input type="checkbox" class="btn-check" id="cbx_Audiologia" autocomplete="off" asp-for="Tab3.EvaluarAudiologia">
                                <label class="cpal-box btn btn-outline-primary d-flex align-items-center p-y4" id="box_Audiologia" onclick="evaluacionSelect('cbx_Audiologia','Audiologia')" for="cbx_Audiologia" style="min-height: 100px">
                                    <a href="JavaScript: void(0);">
                                        <img src="~/assets/images/iconeval/Audiologia.svg" alt="" width="50" height="50" />
                                    </a>                                    
                                    <div class="ms-3 mt-2">
                                        <h5>AUDIOLOGÍA</h5>
                                    </div>
                                    <i class="fa fa-info-circle ms-auto fs-7" data-bs-html="true" data-bs-toggle="tooltip" data-bs-placement="top" aria-label="Audiología" data-bs-original-title="<b>AUDIOLOGÍA</b>:<hr> En el área de Audiología se evalúa el estado auditivo tanto a bebés, niños, jóvenes y adultos, en las diferentes partes del oído: externo, medio y/o interno; ofreciendo un diagnóstico preciso y en caso sea necesario la implementación y adaptación de audífonos, así como programación de implantes cocleares.<br>También se realizan evaluaciones audiológicas para medir el estado del acúfeno; así como la terapia de reentrenamiento auditivo para estos casos.<br> En la etapa escolar y en niños con audición normal, la evaluación de procesamiento auditivo permite identificar las habilidades para diferenciar, reconocer, procesar e interpretar estímulos auditivos." aria-describedby="tooltip794267"></i>
                                </label>
                            </div>
                            <div class="col-md-4 py-2">
                                <input type="checkbox" class="btn-check" id="cbx_Cuerpo" autocomplete="off" asp-for="Tab3.EvaluarCuerpoMovimiento">
                                <label class="cpal-box btn btn-outline-primary d-flex align-items-center p-y4" id="box_Cuerpo" onclick="evaluacionSelect('cbx_Cuerpo','Cuerpo')" for="cbx_Cuerpo" style="min-height: 100px">
                                    <a href="JavaScript: void(0);">
                                        <img src="~/assets/images/iconeval/Cuerpo-y-movimiento-1.svg" alt="" width="50" height="50" />
                                    </a>
                                    <div class="ms-3 mt-2">
                                        <h5>CUERPO Y MOVIMIENTO</h5>
                                    </div>
